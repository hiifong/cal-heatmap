{
  "name": "@hiifong/cal-heatmap",
  "version": "0.0.2",
  "packageManager": "pnpm@9.9.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "main": "dist/lib/index.js",
  "module": "dist/es/index.js",
  "types": "dist/es/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "default": "./dist/es/index.js",
        "types": "./dist/es/index.d.ts"
      },
      "require": {
        "default": "./dist/lib/index.js",
        "types": "./dist/lib/index.d.ts"
      }
    }
  },
  "engines": {
    "node": ">=18.18.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hiifong/cal-heatmap.git"
  },
  "keywords": [
    "cal-heatmap"
  ],
  "author": "hiifong <i@hiif.ong>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hiifong/cal-heatmap/issues"
  },
  "homepage": "https://github.com/hiifong/cal-heatmap#readme",
  "dependencies": {
    "@leafer-in/flow": "^1.0.2",
    "@leafer-ui/interface": "^1.0.2",
    "dayjs": "^1.11.13",
    "leafer-ui": "^1.0.2",
    "leafer-x-tooltip-canvas": "^1.0.0",
    "vue": "^3.4.29"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.8.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/node": "^20.14.5",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.23.0",
    "npm-run-all2": "^6.2.0",
    "prettier": "^3.2.5",
    "typescript": "~5.4.0",
    "unplugin-auto-import": "^0.17.8",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.3.1",
    "vite-plugin-dts": "^4.2.1",
    "vue-tsc": "^2.0.21"
  }
}
